{
	"name": "TodoZen",
	"description": "Todo's extension",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "index.html",
		"default_title": "TodoZen"
	},
	"host_permissions": ["http://localhost:5173/*"],
	"permissions": ["tabs", "storage", "cookies"],
	"icons": {
		"16": "vite.svg",
		"48": "vite.svg",
		"128": "vite.svg"
	}
}
